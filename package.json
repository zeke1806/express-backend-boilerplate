{
  "scripts": {
    "start": "node 'dist/server.js'",
    "build": "tsc -p . && ncp src/typeDefs dist/typeDefs",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
    "prisma:generate": "npx prisma generate",
    "prisma:introspect": "npx prisma introspect",
    "prisma:create_migration": "npx prisma migrate dev --create-only --preview-feature",
    "prisma:migrate": "npx prisma migrate dev --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:fixtures": "npx fixtures ./prisma/fixtures",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "devDependencies": {
    "@getbigger-io/prisma-fixtures-cli": "^1.0.9",
    "@graphql-codegen/cli": "1.19.1",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-upload": "^8.0.4",
    "@types/node": "^14.14.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "^0.0.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "prisma": "^2.24.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@prisma/client": "^2.24.1",
    "apollo-datasource": "^0.8.0",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-upload": "^12.0.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "ncp": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
